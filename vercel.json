{
  "version": 2,
  "outputDirectory": "frontend",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/email-collection",
      "dest": "/email-collection.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/pricing-feedback.html",
      "dest": "/pricing-feedback.html"
    },
    {
      "src": "/health",
      "dest": "/api"
    },
    {
      "src": "/status",
      "dest": "/api"
    },
    {
      "src": "/test",
      "dest": "/api"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 300
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
